   "source": [
    "def solve(n):\n",
    "    if n == 1:\n",
    "        return [1]\n",
    "        \n",
    "    even = n//2\n",
    "    odd = even + n%2\n",
    "    even_arr = solve(even)\n",
    "    odd_arr  = solve(odd)\n",
    "    \n",
    "    return [-1+2*i for i in odd_arr] + [2*i for i in even_arr]\n",
    "    "
   ]
